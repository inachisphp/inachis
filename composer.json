{
    "name": "jedi58/inachis",
    "type": "project",
    "description": "A basic framework for use in producing simple post/page driven websites",
    "license": "proprietary",
    "authors": [
        {
            "name": "David G. Paul"
        }
    ],
    "version": "1.0.0",
    "require": {
        "php": ">=8.4",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5",
        "google/apiclient": "^2.18",
        "jaybizzle/crawler-detect": "^1.2",
        "jfcherng/php-diff": "^6.4",
        "michelf/php-markdown": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "ramsey/uuid": "^4.7",
        "ramsey/uuid-doctrine": "^2.0",
        "symfony/config": "^8.0",
        "symfony/console": "^8.0",
        "symfony/dependency-injection": "^8.0",
        "symfony/doctrine-bridge": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/emoji": "8.*",
        "symfony/flex": "^2.3",
        "symfony/form": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/http-client": "8.*",
        "symfony/http-kernel": "^8.0",
        "symfony/mailer": "8.*",
        "symfony/messenger": "8.*",
        "symfony/monolog-bundle": "^4.0",
        "symfony/password-hasher": "8.*",
        "symfony/property-access": "^8.0",
        "symfony/property-info": "^8.0",
        "symfony/rate-limiter": "8.*",
        "symfony/runtime": "8.*",
        "symfony/security-bundle": "^8.0",
        "symfony/security-csrf": "^8.0",
        "symfony/serializer": "^8.0",
        "symfony/translation": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "symfony/validator": "^8.0",
        "symfony/yaml": "^8.0",
        "twig/extra-bundle": "^3.21",
        "twig/markdown-extra": "^3.22",
        "twig/string-extra": "^3.21"
    },
    "require-dev": {
        "pheromone/phpcs-security-audit": "^2.0",
        "php-coveralls/php-coveralls": "^2.1",
        "php-parallel-lint/php-parallel-lint": "*",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.0",
        "psalm/phar": "^6.13",
        "slevomat/coding-standard": "^8.21",
        "symfony/browser-kit": "^8.0",
        "symfony/css-selector": "^8.0",
        "symfony/error-handler": "^8.0",
        "symfony/maker-bundle": "^1.51",
        "symfony/phpunit-bridge": "^8.0",
        "symfony/stopwatch": "^8.0",
        "symfony/web-profiler-bundle": "^8.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Inachis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inachis\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "create-skeleton": [
            "cp -R vendor/jedi58/inachis/{bin,config,public,templates} .",
            "cp -R vendor/jedi58/inachis/.env.dist .env"
        ],
        "setup-git-hook": [
            "php -r \"if (!file_exists('.git/hooks')) exit(0); copy('dev/git-hooks/pre-commit', '.git/hooks/pre-commit'); chmod('.git/hooks/pre-commit', 0755); echo 'âœ… Installed Git pre-commit hook.\\n';\""
        ],
        "post-create-project-cmd": [
            "@create-skeleton",
            "@auto-scripts"
        ],
        "post-install-cmd": [
            "@auto-scripts",
            "@setup-git-hook"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@setup-git-hook"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C425H726J8GJGXQBKPRWG2F5",
            "allow-contrib": true,
            "docker": true,
            "require": "7.*"
        },
        "google/apiclient-services": [
            "Analytics"
        ]
    }
}
