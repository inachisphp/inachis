{
    "name": "jedi58/inachis",
    "type": "project",
    "description": "A basic framework for use in producing simple post/page driven websites",
    "license": "proprietary",
    "authors": [
        { "name": "David G. Paul" }
    ],
    "version": "0.0.5",
    "require": {
        "php": ">=7.2.0",
        "ext-iconv": "*",
        "ext-json": "*",
        "aptoma/twig-markdown": "^3.0",
        "michelf/php-markdown": "^1.8",
        "sensio/framework-extra-bundle": "^5.3",
        "shivas/versioning-bundle": "^3.1",
        "symfony/console": "^5.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^5.0",
        "symfony/security-csrf": "^5.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/translation": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "easycorp/easy-deploy-bundle": "^1.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "pheromone/phpcs-security-audit": "^2.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "sensiolabs/security-checker": "^6.0",
        "symfony/debug": "^4.4",
        "symfony/dotenv": "^5.0",
        "symfony/maker-bundle": "^1.8",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "create-skeleton": [
            "cp -R vendor/jedi58/inachis/{bin,config,public} .",
            "cp -R vendor/jedi58/inachis/.env.dist .env"
        ],
        "post-create-project-cmd": [
            "@create-skeleton",
            "@auto-scripts"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C425H726J8GJGXQBKPRWG2F5",
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
