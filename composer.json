{
    "name": "jedi58/inachis",
    "type": "library",
    "description": "A basic framework for use in producing simple post/page driven websites",
    "keywords": ["dev"],
    "license": "MIT",
    "authors": [
        { "name": "David G. Paul" }
    ],
    "require": {
        "php": ">=5.5",
        
        "doctrine/dbal": "2.*",
        "doctrine/orm": "2.*",
        "klein/klein": "^2.1",
        "monolog/monolog": "1.*",
        "paragonie/halite": "1.*",
        "twig/twig": "1.*",
        "aptoma/twig-markdown": "^2.0",
        "michelf/php-markdown": "^1.6",
        "paragonie/random_compat": "^2.0"
    },
    "require-dev": {
        "behat/behat": "2.*",
        "mockery/mockery": "*",
        "phpdocumentor/phpdocumentor": "2.*",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Inachis\\Component\\": "src/Inachis/"
        }
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "archive": {
        "exclude": [ "tests" ]
    }
}
