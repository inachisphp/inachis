<meta name="twitter:card" content="summary_large_image">
{% if settings.twitter|default('') is not empty %}
<meta name="twitter:site" content="@{{ settings.twitter }}">
<meta name="twitter:creator" content="@{{ settings.twitter }}">
{% endif %}
<meta name="twitter:title" content="{{ page.title }}{% if page.title != '' %} / {% endif %}{{ settings.siteTitle }}">
<meta name="twitter:description" content="{{ page.description }}">
{% if post.featureImage|default('') is not empty %}
<meta name="twitter:image" content="{{ post.featureImage }}">
<meta name="twitter:image:alt" content="">
{% endif %}


<meta property="og:site_name" content="{{ settings.siteTitle }}">
<meta property="og:url" content="{{ page.self }}">
<meta property="og:type" content="website">
<meta property="og:title" content="{{ page.title }}{% if page.title != '' %} / {% endif %}{{ settings.siteTitle }}">
<meta property="og:description" content="{{ page.description }}">
{% if post.featureImage|default('') is not empty %}
<meta property="og:image" content="{{ post.featureImage }}">
<!-- meta property="og:image:type" content="image/jpeg" -->
<meta property="og:image:alt" content="">
{% endif %}
<meta property="og:article:author" content="David G. Paul">
<meta property="og:article:published_time" content="">
{% if settings.fb_app_id|default('') is not empty %}
    <meta property="fb:app_id" content="{{ settings.fb_app_id }}">
{% endif %}
