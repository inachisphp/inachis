{% block metadata %}
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <meta http-equiv="keywords" content="{{ page.keywords }}" />
    <meta http-equiv="Content-Style-Type" content="text/css" />
    <meta http-equiv="Last-Update" content="{{ page.modDate }}" />
    <meta http-equiv="Content-Language" content="{{ settings.language }}" />
    <meta http-equiv="imagetoolbar" content="no" />
    <meta name="Author" content="David G. Paul" />
    <meta name="Copyright" content="&copy; David G. Paul" />
    <meta name="keywords" content="{{ page.keywords }}" />
    <meta name="description" content="{{ page.description }}" />
    <meta name="abstract" content="" />
    <meta name="category" content="home page" />
    <meta name="robots" content="index,follow" />
    <meta name="googlebot" content="NOODP" />
    <meta name="language" content="{{ settings.language }}" />
    <meta name="revised" content="{{ page.modDate }}" />
{% if settings.geo.lat|default('') is not empty and settings.geo.long|default('') is not empty %}
    <meta name="ICBM" content="{{ settings.geo.lat }}, {{ settings.geo.long }}" />
    <meta name="geo.position" content="{{ settings.geo.lat }}, {{ settings.geo.long }}" />
{% endif %}
{% if settings.geo.name|default('') is not empty %}
    <meta name="geo.placename" content="{{ settings.geo.name }}" />
{% endif %}
{% if settings.geo.region|default('') is not empty %}
    <meta name="geo.region" content="{{ settings.geo.region }}" />
{% endif %}
    <link rel="schema.DC" href="http://purl.org/dc/elements/1.1/" />
    <link rel="schema.DCTERMS" href="http://purl.org/dc/terms/" />
    <meta name="DC.title" content="{{ settings.siteTitle }}" />
    <meta name="DC.creator" content="David G. Paul" />
    <meta name="DC.subject" content="" />
    <meta name="DC.description" content="{{ page.description }}" />
    <meta name="DC.type" scheme="DCTERMS.DCMIType" content="Text" />
    <meta name="DC.format" content="text/html" />
    <meta name="DC.identifier" scheme="DCTERMS.URI" content="{{ app.request.uri }}" />
    <meta name="generator" content="inachis" />

    <link rel="apple-touch-icon" sizes="57x57" href="{{ settings.domain }}/assets/imgs/favicons/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="{{ settings.domain }}/assets/imgs/favicons/apple-touch-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ settings.domain }}/assets/imgs/favicons/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ settings.domain }}/assets/imgs/favicons/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ settings.domain }}/assets/imgs/favicons/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ settings.domain }}/assets/imgs/favicons/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ settings.domain }}/assets/imgs/favicons/apple-touch-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ settings.domain }}/assets/imgs/favicons/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ settings.domain }}/assets/imgs/favicons/apple-touch-icon-180x180.png">
    <meta name="apple-mobile-web-app-title" content="{{ 'siteTitle' }}">
    <link rel="mask-icon" href="/inadmin/assets/images/favicons/safari-pinned-tab.svg" color="#5bbad5">

    <link rel="icon" type="image/png" href="{{ settings.domain }}/assets/imgs/favicons/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="{{ settings.domain }}/assets/imgs/favicons/favicon-16x16.png" sizes="16x16">
    <link rel="icon" type="image/png" href="{{ settings.domain }}/assets/imgs/favicons/favicon-96x96.png" sizes="96x96">

    <link rel="manifest" href="{{ settings.domain }}/assets/imgs/favicons/manifest.json">
    <link rel="icon" type="image/png" href="{{ settings.domain }}/assets/imgs/favicons/android-chrome-192x192.png" sizes="192x192">
    <meta name="theme-color" content="#069">

    <meta name="application-name" content="{{ settings.siteTitle }}">
    <meta name="msapplication-TileColor" content="#2d89ef">
    <meta name="msapplication-TileImage" content="{{ settings.domain }}/assets/imgs/windows10-144x144.png">

    <link rel="canonical" href="/" />
    <link rel="search" type="application/opensearchdescription+xml" title="{{ settings.siteTitle }}" href="{{ settings.domain }}/search.xml" />
    <link rel="Shortcut Icon" type="image/x-icon" href="{{ settings.domain }}/favicon.ico" />
    <link rel="icon" type="image/x-icon" href="{{ settings.domain }}/favicon.ico" />

{% include "web/structure/_metadata__open-graph.html.twig" %}

{% endblock metadata %}
